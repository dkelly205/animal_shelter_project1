<main>
<h2>Customers</h2>



<% @customers.each do |customer| %>

<p>First Name: <%= customer.first_name() %> </p>
<p>Last Name: <%= customer.last_name() %> </p>
<p>Address: <%= customer.address() %> </p>
<p>Phone Number: <%= customer.phone_number %> </p>

<% end %>


<a href="/customers/new">Add Customer</a>
